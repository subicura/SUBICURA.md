<body>
  <div class="container">
    {% if backLink %}
    <a href="/" class="back">← cd ..</a>
    {% endif %}

    <header>
      <h1>{{ heading }}<span class="version">{{ version }}</span></h1>
      <p class="subtitle">{{ subtitle }}</p>
    </header>

    {{ content | safe }}

    <footer>
      <p>
        <a href="/deps/" onclick="posthog.capture('click_deps_link')">deps</a> · <a href="/til/" onclick="posthog.capture('click_til_link')">til</a> · <a href="/help/" onclick="posthog.capture('click_help_link')">help</a> · <a href="https://x.com/subicura" target="_blank" rel="noopener" onclick="posthog.capture('click_twitter_link')">@subicura</a>
